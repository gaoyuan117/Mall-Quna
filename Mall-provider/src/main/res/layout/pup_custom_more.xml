<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <LinearLayout 
        android:id="@+id/llayout_pup_more_identify"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/black"
        android:padding="10dp"
        >
        <TextView 
            style="@style/Text_Wrap_White_Text_Title2"
            android:text="正在鉴定"
            />
    </LinearLayout>
    
    <LinearLayout 
        android:id="@+id/llayout_pup_more_shipments"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/black"
        android:padding="10dp"
        android:layout_marginTop="0.5dp"
        >
        <TextView 
            style="@style/Text_Wrap_White_Text_Title2"
            android:text="趣那已发货"
            />
    </LinearLayout>
    
    <LinearLayout 
        android:id="@+id/llayout_pup_more_evaluate"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/black"
        android:padding="10dp"
        android:layout_marginTop="0.5dp"
        >
        <TextView 
            style="@style/Text_Wrap_White_Text_Title2"
            android:text="待评价"
            />
    </LinearLayout>
    
    <LinearLayout 
        android:id="@+id/llayout_pup_more_complete"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/black"
        android:padding="10dp"
        android:layout_marginTop="0.5dp"
        >
        <TextView 
            style="@style/Text_Wrap_White_Text_Title2"
            android:text="已完成"
            />
    </LinearLayout>
    
    <LinearLayout 
        android:id="@+id/llayout_pup_more_finish"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/black"
        android:padding="10dp"
        android:layout_marginTop="0.5dp"
        >
        <TextView 
            style="@style/Text_Wrap_White_Text_Title2"
            android:text="已关闭"
            />
    </LinearLayout>

</LinearLayout>
